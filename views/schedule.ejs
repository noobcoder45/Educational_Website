<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <title></title>
  </head>
  <body>
    <h1>Schedule</h1>
    <hr>
    <table>
  <tr>
    <th>Date</th>
    <th>Time</th>
    <th>Description</th>
    <th>Link</th>
  </tr>
  <% if(item.group === 0){ %>
  <% meetings.forEach(function(item){ %>

    <%  if(item.students.includes(id)){ %>
    <tr>
      <td><%= item.date %></td>
      <td><%= item.time %></td>
      <td><%= item.description %></td>
      <td><a href=<%= item.link %> class="list-group-item"><%= item.link %></a></td>
    </tr>
    <% } %>
  <% } %>
  <% } %>
  <% if(item.students[0]==="none"){ %>
    <% meetings.forEach(function(item){ %>
    <% if(item.group===class){ %>
    <tr>
      <td><%= item.date %></td>
      <td><%= item.time %></td>
      <td><%= item.description %></td>
      <td><a href=<%= item.link %> class="list-group-item"><%= item.link %></a></td>
    </tr>
  <% } %>
  <% }) %>
  <% } %>
</table>
  </body>
</html>
